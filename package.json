    "express-validator": "^6.9.2",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "stylelint": "^13.10.0"
  }
